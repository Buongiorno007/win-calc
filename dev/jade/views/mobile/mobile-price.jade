.mobile-price(ng-controller="ConfigMenuCtrl as configMenu")
  price(price-value="configMenu.G.global.tempPrice", price-currency="configMenu.U.userInfo.currency")
  .go-to-cart(ng-click='configMenu.checkForAddElem()')
    .go-to-cart-title {{configMenu.CONFIGMENU_IN_CART}}
    .product-counter {{configMenu.G.global.product_qty}}