.config-panel.profile-container(ng-show="profilesPanel.U.userInfo.factory_id !== 2", ng-controller="ProfilesCtrl as profilesPanel", ng-class="{'active': profilesPanel.G.global.activePanel == 2}")
    .panel-inner(ng-if="profilesPanel.G.global.activePanel == 2")
        .profile-block.clearfix(ng-repeat="producer in profilesPanel.G.global.profiles track by $index", ng-class="{'selected': profilesPanel.P.product.profileTypeIndex == $index}")
            .group-block-header(ng-if="profilesPanel.G.global.profilesType[$index].name !== 'Default'")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'en'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.en }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'ru'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.ru }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'uk'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.ua }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'pl'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.pl }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'de'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.de }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'bg'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.bg }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'it'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.it }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'es'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.es }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'ro'", data-output="{{ profilesPanel.G.global.profilesType[$index].translate.ro }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")

                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'en' && !profilesPanel.G.global.profilesType[$index].translate.en", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'ru' && !profilesPanel.G.global.profilesType[$index].translate.ru", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'uk' && !profilesPanel.G.global.profilesType[$index].translate.ua", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'pl' && !profilesPanel.G.global.profilesType[$index].translate.pl", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'de' && !profilesPanel.G.global.profilesType[$index].translate.de", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'bg' && !profilesPanel.G.global.profilesType[$index].translate.bg", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'it' && !profilesPanel.G.global.profilesType[$index].translate.it", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'es' && !profilesPanel.G.global.profilesType[$index].translate.es", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .title(ng-if="profilesPanel.U.userInfo.langLabel === 'ro' && !profilesPanel.G.global.profilesType[$index].translate.ro", data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .group-info
                    .info(ng-if="profilesPanel.G.global.profilesType[$index].img.length || profilesPanel.G.global.profilesType[$index].link.length || profilesPanel.G.global.profilesType[$index].description", ng-click="profilesPanel.showInfoBox(profilesPanel.G.global.profilesType[$index].id, profilesPanel.G.global.profilesType)")
            .box.profile-box.unvisible(ng-repeat="profile in producer track by $index", ng-class="{'selected': profile.id == profilesPanel.P.product.profile.id}", show-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_BLOCK * $index) }}")
                .box_inner(ng-click="profilesPanel.checkForAddElem(profile.id)")
                    img.profile-img(ng-if="profile.img.length", ng-src="{{profile.img}}")

                    .profile-country(ng-if="!profile.img.length",data-output="{{ profile.country }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")
                    .label-country(ng-if="!profile.img.length",data-output="{{ ::profilesPanel.COUNTRY }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")
                    //-Conditions to display different translations
                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'en'", data-output="{{ profile.translate.en }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'ru'", data-output="{{ profile.translate.ru }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'uk'", data-output="{{ profile.translate.ua }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'pl'", data-output="{{ profile.translate.pl }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'it'", data-output="{{ profile.translate.it }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'de'", data-output="{{ profile.translate.de }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'bg'", data-output="{{ profile.translate.bg }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")
                    
                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'es'", data-output="{{ profile.translate.es }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'ro'", data-output="{{ profile.translate.ro }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    //-If there are no translation of the language it's displaying default value
                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'en' && !profile.translate.en", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'ru' && !profile.translate.ru", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'uk' && !profile.translate.ua", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'pl' && !profile.translate.pl", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'it' && !profile.translate.it", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'de' && !profile.translate.de", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'bg' && !profile.translate.bg", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'es' && !profile.translate.es", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-descr(ng-if="profilesPanel.U.userInfo.langLabel === 'ro' && !profile.translate.ro", data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-cameras(ng-if="!profile.img.length",)
                        .type.cam {{ profile.cameras }}
                        .type-label
                            ng-pluralize(count="profile.cameras",when="{'1':'{{ profilesPanel.config.camera }}', '2':'{{ profilesPanel.config.camer }}', '3':'{{ profilesPanel.config.camer }}', '4':'{{ profilesPanel.config.camer }}', 'other':'{{ profilesPanel.config.camers }}'}")
                    .profile-box-bottom
                        .label(data-output="{{ ::profilesPanel.HEAT_INSULATION }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")
                        .coef-level.heat-insulation(ng-class="{'coef-level-1': profile.heat_coeff == 1, 'coef-level-2': profile.heat_coeff == 2, 'coef-level-3': profile.heat_coeff == 3, 'coef-level-4': profile.heat_coeff == 4}")
                            .coef-index
                            .coef-index
                            .coef-index
                            .coef-index
                            .coef-index
                        .label(data-output="{{ ::profilesPanel.NOICE_INSULATION }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")
                        .coef-level.noise-insulation(ng-class="{'coef-level-1': profile.noise_coeff == 1, 'coef-level-2': profile.noise_coeff == 2, 'coef-level-3': profile.noise_coeff == 3, 'coef-level-4': profile.noise_coeff == 4}")
                            .coef-index
                            .coef-index
                            .coef-index
                            .coef-index
                            .coef-index
                .info(ng-if="profile.img.length || profile.link.length || profile.description", ng-click="profilesPanel.showInfoBox(profile.id, producer)")



.config-panel.profile-container(ng-show="profilesPanel.U.userInfo.factory_id === 2", ng-controller="ProfilesCtrl as profilesPanel", ng-class="{'active': profilesPanel.G.global.activePanel == 2}")
    .panel-inner(ng-if="profilesPanel.G.global.activePanel == 2")
        .profile-block.clearfix(ng-repeat="producer in profilesPanel.G.global.profiles track by $index", ng-class="{'selected': profilesPanel.P.product.profileTypeIndex == $index}")
            .group-block-header-rehau(ng-if="profilesPanel.G.global.profilesType[$index].name !== 'Default'")
                .title-rehau(data-output="{{ profilesPanel.G.global.profilesType[$index].name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START }}")
                .group-info-rehau
                    img.info-icon(ng-src="/img/rehau-img/info_rehau.png", ng-if="profilesPanel.G.global.profilesType[$index].img.length || profilesPanel.G.global.profilesType[$index].link.length || profilesPanel.G.global.profilesType[$index].description", ng-click="profilesPanel.showInfoBox(profilesPanel.G.global.profilesType[$index].id, profilesPanel.G.global.profilesType)")
                    
            .box-rehau.profile-box.unvisible(ng-repeat="profile in producer track by $index", ng-class="{'selected': profile.id == profilesPanel.P.product.profile.id}", show-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_BLOCK * $index) }}")
                .box_inner-rehau(ng-click="profilesPanel.checkForAddElem(profile.id)")
                    img.profile-img(ng-if="profile.img.length", ng-src="{{profile.img}}")

                    .profile-country(ng-if="!profile.img.length",data-output="{{ profile.country }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")
                    .label-country(ng-if="!profile.img.length",data-output="{{ ::profilesPanel.COUNTRY }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")
                    //-Conditions to display different translations
                    .profile-descr-rehau(data-output="{{ profile.name }}", typing="{{ profilesPanel.config.typing }}", typing-delay="{{ profilesPanel.config.DELAY_START + (profilesPanel.config.DELAY_TYPING * $index) }}")

                    .profile-cameras(ng-if="!profile.img.length",)
                        .type.cam {{ profile.cameras }}
                        .type-label
                            ng-pluralize(count="profile.cameras",when="{'1':'{{ profilesPanel.config.camera }}', '2':'{{ profilesPanel.config.camer }}', '3':'{{ profilesPanel.config.camer }}', '4':'{{ profilesPanel.config.camer }}', 'other':'{{ profilesPanel.config.camers }}'}")
                    .profile-box-bottom-rehau
                        .profile-blocks
                            .profile-block-noise-rehau
                                .profile-gray-block
                                    .profile-text-block <span>Шумоизоляция</span>
                                    .profile-number-block <span>7</span>
                                .profile-red-block
                            .profile-block-light-rehau
                                .profile-gray-block
                                    .profile-text-block <span>Светопропускание</span>
                                    .profile-number-block <span>7</span>
                                .profile-red-block
                            .profile-block-warm-rehau
                                .profile-gray-block
                                    .profile-text-block <span>Теплоизоляция</span>
                                    .profile-number-block <span>7</span>
                                .profile-red-block
                            .profile-block-burglary-rehau
                                .profile-gray-block
                                    .profile-text-block <span>Взломостойкость</span>
                                    .profile-number-block <span>7</span>
                                .profile-red-block
                                .profile-info-block(ng-if="profile.img.length || profile.link.length || profile.description", ng-click="profilesPanel.showInfoBox(profile.id, producer)") <span>Подробнее</span>
                        .profile-blocks-green-block
                                   
                                     
                //- .info(ng-if="profile.img.length || profile.link.length || profile.description", ng-click="profilesPanel.showInfoBox(profile.id, producer)")