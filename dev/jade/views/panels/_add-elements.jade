.config-panel.auxiliaries-container(ng-controller="AddElementsCtrl as elementsPanel", ng-class="{'active': elementsPanel.G.global.activePanel == 6}")
  .auxiliaries-inner(ng-if="elementsPanel.G.global.activePanel == 6")

    .auxiliary-item.aux-outside.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_OUTSIDE }}")
      .aux-template2-img.move-outside(ng-show="elementsPanel.G.global.typeMenu === 2222")
      .aux-template1-img.move-outside(ng-show="elementsPanel.G.global.typeMenu === 1111 || elementsPanel.G.global.typeMenu === 3333 || elementsPanel.G.global.typeMenu === 5555")

    .auxiliary-item.unvisible(
      ng-repeat="addElemGroup in elementsPanel.P.product.chosenAddElements track by $index",
      ng-if="elementsPanel.G.global.addElementsAll[$index].elementsList && elementsPanel.config.addElementDATA[$index].mainTypeMenu === elementsPanel.G.global.typeMenu || elementsPanel.G.global.addElementsAll[$index].elementsList && elementsPanel.config.addElementDATA[$index].typeMenu === elementsPanel.G.global.typeMenu",
      ng-switch="$index",
      show-delay="{{ elementsPanel.config.addElementDATA[$index].delay }}",
      ng-class="[elementsPanel.config.addElementDATA[$index].typeClass, elementsPanel.config.addElementDATA[$index].typeClass + '-theme', {'focused': elementsPanel.A.aux.isFocusedAddElement == ($index+1) && !addElemGroup.length, 'selected': addElemGroup.length}]")

      .aux-grid-img.move-grid(ng-switch-when="0")
      .aux-visor-img.move-visor(ng-switch-when="1")
      .aux-spillway-img.move-spillway(ng-switch-when="2")
      .aux-outside-img.move-outside(ng-switch-when="3")
      .aux-louver-img.move-louvers(ng-switch-when="4")
        .aux-louver-top
        .aux-louver-midle.louver-midle-1.move-louver-midle-1
        .aux-louver-midle.louver-midle-2.move-louver-midle-2
        .aux-louver-midle.louver-midle-3.move-louver-midle-3
        .aux-louver-bottom.move-louver-bottom
      .aux-inside-img(ng-switch-when="5")
        .aux-inside-top.move-inside-top.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_INSIDESLOPETOP }}")
        .aux-inside-right.move-inside-right.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_INSIDESLOPERIGHT }}")
        .aux-inside-left.move-inside-left.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_INSIDESLOPELEFT }}")
      .aux-connectors-img(ng-switch-when="6")
        .aux-force-connect.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_FORCECONNECT }}")
        .aux-balcon-connect.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_BALCONCONNECT }}")
      .aux-fan-img.move-fan(ng-switch-when="7")
      .aux-windowsill-img.move-windowsill(ng-switch-when="8")
      .aux-handle-img.move-handle(ng-switch-when="9")
      .aux-txt-box.aux-others-txt(ng-switch-when="10")
      .aux-shutters-img.move-shutters(ng-switch-when="11")
      .aux-grating-img.move-grating(ng-switch-when="12")
      .aux-blind-img.move-blind(ng-switch-when="13")
      .aux-shut-img.move-shut(ng-switch-when="14")
      .aux-grat-img.move-grat(ng-switch-when="15")
      .aux-vis-img.move-vis(ng-switch-when="16")
      .aux-spil-img.move-spil(ng-switch-when="17")

      .aux-txt-box(ng-class="elementsPanel.config.addElementDATA[$index].typeClass + '-txt'")
        .choose-block.unvisible(ng-hide="addElemGroup.length", show-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}")
          button.aux-choose-but(ng-click="elementsPanel.selectAddElement($index+1)") {{ ::elementsPanel.CHOOSE }}
          .aux-label(data-output="{{ elementsPanel.config.addElementDATA[$index].name | translate }}", typing="{{ elementsPanel.config.typing }}", typing-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}")

        .selected-block(ng-show="addElemGroup.length", ng-class="($index==1)?'horisontal':'vertival'")
          .aux-info-box
            .aux-title(data-output="{{ addElemGroup[0].name }}", fs-click="elementsPanel.selectAddElement($index+1)", typing="{{ elementsPanel.config.typing }}", typing-delay="{{ elementsPanel.config.DELAY_START }}")
            .aux-label(data-output="{{ elementsPanel.config.addElementDATA[$index].name | translate }}", typing="{{ elementsPanel.config.typing }}", typing-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}")

          .aux-sizing-box
            .aux-params(ng-class="{'selected': elementsPanel.A.aux.auxParameter === ($index+1)+'-1-0'}")
              button.aux-params-but(fs-click="elementsPanel.initAddElementTools(($index+1), 1, 0)", ng-keyup="elementsPanel.pressCulculator($event)", ng-disabled="!elementsPanel.P.product.is_addelem_only && $index == 0", ng-class="{'color-selected': !elementsPanel.P.product.is_addelem_only && $index == 0}") {{ addElemGroup[0].element_qty }}
              .aux-label(data-output="{{ ::elementsPanel.QTY_LABEL }}", typing="{{ elementsPanel.config.typing }}", typing-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}")
            .aux-params(ng-hide="$index==3 || $index==5 || $index==7 || $index==9 || $index==10", ng-class="{'selected': elementsPanel.A.aux.auxParameter === ($index+1)+'-2-0'}")
              button.aux-params-but(fs-click="elementsPanel.initAddElementTools(($index+1), 2, 0)", ng-keyup="elementsPanel.pressCulculator($event)", ng-disabled="!elementsPanel.P.product.is_addelem_only && $index == 0", ng-class="{'color-selected': !elementsPanel.P.product.is_addelem_only && $index == 0}")  {{ addElemGroup[0].element_width }}
              .aux-label(data-output="{{ ::elementsPanel.WIDTH_LABEL }}", typing="{{ elementsPanel.config.typing }}", typing-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}")
            .aux-params(ng-show="!$index || $index==4", ng-class="{'selected': elementsPanel.A.aux.auxParameter === ($index+1)+'-3-0'}")
              button.aux-params-but(fs-click="elementsPanel.initAddElementTools(($index+1), 3, 0)", ng-keyup="elementsPanel.pressCulculator($event)", ng-disabled="!elementsPanel.P.product.is_addelem_only && $index == 0", ng-class="{'color-selected': !elementsPanel.P.product.is_addelem_only && $index == 0}")  {{ addElemGroup[0].element_height }}
              .aux-label(data-output="{{ ::elementsPanel.HEIGHT_LABEL }}", typing="{{ elementsPanel.config.typing }}", typing-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}")
          .aux-more(ng-show="addElemGroup.length>1") {{ ::elementsPanel.OTHER_ELEMENTS1 }} {{ addElemGroup.length - 1 }} {{ ::elementsPanel.OTHER_ELEMENTS2 }}

    .numberMenu(fs-click="elementsPanel.click(5555); elementsPanel.G.global.isServiceCalculator = 0; ", ng-class="{'green': elementsPanel.config.colorFilter===5555}", id="5555") {{::elementsPanel.ALL}}
    .numberMenu(fs-click="elementsPanel.click(1111); elementsPanel.G.global.isServiceCalculator = 0; ", ng-class="{'green': elementsPanel.config.colorFilter===1111}", id="1111") {{::elementsPanel.INSIDES}}
    .numberMenu(fs-click="elementsPanel.click(2222); elementsPanel.G.global.isServiceCalculator = 0; ", ng-class="{'green': elementsPanel.config.colorFilter===2222}", id="2222") {{::elementsPanel.OUTSIDES}}
    .numberMenu(fs-click="elementsPanel.click(3333); elementsPanel.G.global.isServiceCalculator = 0; ", ng-class="{'green': elementsPanel.config.colorFilter===3333}", id="3333") {{::elementsPanel.COMPONENTS}}
    .numberMenu(fs-click="elementsPanel.click(4444); elementsPanel.G.global.isServiceCalculator = 0; ", ng-class="{'green': elementsPanel.config.colorFilter===4444}", id="4444") {{::elementsPanel.OTHER}}
    .numberMenu(fs-click="elementsPanel.click(6666)", ng-class="{'green': elementsPanel.config.colorFilter===6666}", id="6666") {{::elementsPanel.SERVICES}}

    .services(ng-if="elementsPanel.G.global.typeMenuID === 6666")
      .service-block
        .service-type(ng-class="{'green': elementsPanel.G.global.servicesPriceIndex===0}")
          |Заделка внешнего шва
        label.service-input(ng-click="elementsPanel.openServiceCalculator(0)"  data-bind="click: function() {clearDisplay();}" )
         | {{elementsPanel.P.product.servicesPrice[0] | number : 2}}
      .service-block
        .service-type(ng-class="{'green': elementsPanel.G.global.servicesPriceIndex===1}")
          |Заделка шва под подоконником
        label.service-input(ng-click="elementsPanel.openServiceCalculator(1)"  data-bind="click: function() {clearDisplay();}")
          | {{elementsPanel.P.product.servicesPrice[1] | number : 2}}
      .service-block
        .service-type( ng-class="{'green': elementsPanel.G.global.servicesPriceIndex===2}")
          |Подготовительне работы
        label.service-input(ng-click="elementsPanel.openServiceCalculator(2)"  data-bind="click: function() {clearDisplay();}")
          | {{elementsPanel.P.product.servicesPrice[2] | number : 2}}
      .service-block
        .service-type(ng-class="{'green': elementsPanel.G.global.servicesPriceIndex===3}")
          |Отделка внутренних откосов
        label.service-input(ng-click="elementsPanel.openServiceCalculator(3)"  data-bind="click: function() {clearDisplay();}")
          | {{elementsPanel.P.product.servicesPrice[3] | number : 2}}
      .service-block
        .service-type(ng-class="{ 'green': elementsPanel.G.global.servicesPriceIndex===4}")
          |Прочее
        label.service-input(ng-click="elementsPanel.openServiceCalculator(4)"  data-bind="click: function() {clearDisplay();}")
          | {{elementsPanel.P.product.servicesPrice[4] | number : 2}}

    button.scheme-info.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}", fs-click="elementsPanel.showWindowScheme()")

    .scheme-view-dialog(ng-class="{'active': elementsPanel.A.aux.isWindowSchemeDialog}")
      svg-template(template="elementsPanel.P.product.template", template-width="elementsPanel.constants.svgTemplateIconBigWidth", template-height="elementsPanel.constants.svgTemplateIconBigHeight", type-construction="elementsPanel.constants.SVG_ID_ICON")
      .close-dialog(fs-click="elementsPanel.closeWindowScheme()")

    button.view-switch-tab.unvisible(show-delay="{{ elementsPanel.config.DELAY_SHOW_BUTTON }}", fs-click="elementsPanel.openAddElementListView()") {{ 'add_elements.LIST_VIEW' | translate }}