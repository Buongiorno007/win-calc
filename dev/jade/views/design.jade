      .door-config-column.step1
        .door-config-backfon
        .door-config-header
          .door-inner
            .door-title 1 {{ ::designPage.STEP }}
            .door-subtitle {{ ::designPage.LABEL_DOOR_TYPE }}
        .door-config-content(ng-class="{'scroll-box': !designPage.D.design.steps.selectedStep2}")
