    .elements-menu
        header.menu-header
            .tip {{ ::elementsMenu.TIP }}
            button.close-elements-menu(fs-click="elementsMenu.closeAddElementsMenu()")

        .menu-lists
            ul.items-lister(ng-if="elementsMenu.G.global.currOpenPage === 'main'")
