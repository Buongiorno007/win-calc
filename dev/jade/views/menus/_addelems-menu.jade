            ul.items-lister(ng-if='elementsMenu.A.aux.addElementsType[$index].name === "Secustik Roto Swing"' ng-repeat="type in elementsMenu.A.aux.addElementsList track by $index", id="{{$index}}", ng-class="{'one-elem': elementsMenu.A.aux.addElementsList.length<=1}")
                .title(ng-click="elementsMenu.hideMenu($index)") {{ ::elementsMenu.MENU_ELEMENT }}
                    button.element-info(ng-if="elementsMenu.A.aux.addElementsType[$index].img.length || elementsMenu.A.aux.addElementsType[$index].link.length || elementsMenu.A.aux.addElementsType[$index].description.length", fs-click="elementsMenu.showInfoBox(elementsMenu.A.aux.addElementsType[$index].id, elementsMenu.A.aux.addElementsType)")
                    .open-list(id="{{$index + 'open'}}")
                        img(ng-src='img/plus.png')
                    .open-list2(id="{{$index + 'close'}}")
                        img(ng-src='img/minus.png')
                li.item(ng-repeat="addElement in type track by $index")
                    .value(ng-click="elementsMenu.selectAddElement($parent.$index, $index, $event)", id="{{$parent.$index+'prod'+$index}}") {{ addElement.name }}
