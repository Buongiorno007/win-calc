.config-menu(ng-controller="ConfigMenuCtrl as configMenu")
  ul.items-list.unvisible(ng-if="configMenu.G.global.isConfigMenu", show-delay="{{ configMenu.config.DELAY_SHOW_CONFIG_LIST }}")

    li.item.item-header(ng-class="{'active': configMenu.G.global.activePanel == 1, 'hov': configMenu.G.global.activePanel !== 1}")
      .title(data-output="{{ ::configMenu.CONFIGMENU_CONFIGURATION }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}",fs-click="configMenu.selectConfigPanel(1)")
      .icon.unvisible(ng-if="configMenu.G.global.isConfigMenu", show-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}",fs-click="configMenu.selectConfigPanel(1)")
        svg-template(template="configMenu.P.product.templateIcon", template-width="configMenu.constants.svgTemplateIconWidth", template-height="configMenu.constants.svgTemplateIconHeight", type-construction="configMenu.constants.SVG_CLASS_ICON")
      //.right-desc
      //  .name(data-output="{{ ::configMenu.CONFIGMENU_SIZING }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}",fs-click="configMenu.selectConfigPanel(1)")
      //  .value(data-output="{{ configMenu.P.product.template_width }} {{ ::configMenu.MM }}* ", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}",fs-click="configMenu.selectConfigPanel(1)")
      //  .value(data-output="{{ configMenu.P.product.template_height }} {{ ::configMenu.MM }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}",fs-click="configMenu.selectConfigPanel(1)")
      .counter-main-page
        .counter-block
          p  {{configMenu.COUNT}}
          .counter
            .input-minus(ng-click="configMenu.subtractProdQty()") -
            input.prodCounter(type="number", min="1", ng-model="configMenu.G.global.product_qty",  ng-keyup="configMenu.setCount()" readonly)
            .input-plus(ng-click="configMenu.addProdQty()") +
      //.value(data-output="{{ configMenu.P.product.template_height }} {{ ::configMenu.MM }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")

    li.item(ng-class="{'active': configMenu.G.global.activePanel == 2, 'hov': configMenu.G.global.activePanel !== 2}", fs-click="configMenu.selectConfigPanel(2)")
