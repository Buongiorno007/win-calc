      .value(ng-if="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'es'", data-output="{{ configMenu.P.product.hardware.translate.es }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-if="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'ro'", data-output="{{ configMenu.P.product.hardware.translate.ro }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")

