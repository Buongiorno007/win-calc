      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'ru' && !configMenu.P.product.profile.translate.ru", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'uk' && !configMenu.P.product.profile.translate.ua", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'pl' && !configMenu.P.product.profile.translate.pl", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'it' && !configMenu.P.product.profile.translate.it", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'de' && !configMenu.P.product.profile.translate.de", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'bg' && !configMenu.P.product.profile.translate.bg", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'es' && !configMenu.P.product.profile.translate.es", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
      .value(ng-show="configMenu.P.product.construction_type !== 4 && configMenu.U.userInfo.langLabel === 'ro' && !configMenu.P.product.profile.translate.ro", data-output="{{ configMenu.P.product.profile.name }}", typing="{{ configMenu.config.typing }}", typing-delay="{{ configMenu.config.DELAY_TYPE_ITEM_TITLE }}")
