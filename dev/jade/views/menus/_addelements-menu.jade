            .aux-label {{ 'add_elements.SIZE_LABEL' | translate }}
          .aux-params(ng-if="addElement.elementColor")
            button.aux-color
              img(ng-src="{{ addElement.elementColor }}")
            .aux-label {{ 'add_elements.COLOR_LABEL' | translate }}

          .aux-price +{{ addElement.elementPrice }}
          button.aux-choose-but(hm-tap="elementsMenu.chooseAddElement($parent.$index, $index)") {{ 'add_elements.ADD' | translate }}



