.elements-list-container(ng-controller="addElementMenuCtrl as elementsMenu", ng-class="[elementsMenu.aux.showAddElementsMenu, elementsMenu.aux.addElementsMenuStyle]")
  .elements-menu
    header.menu-header
      .tip(data-output="{{ 'add_elements_menu.TIP' | translate }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")
      button.close-elements-menu(hm-tap="elementsMenu.closeAddElementsMenu()")

    .menu-lists
      ul.items-list
        li.item.empty-element(ng-class="{'active': elementsMenu.aux.isAddElement == false}", hm-tap="elementsMenu.chooseAddElement()")
          .value(data-output="{{ 'add_elements_menu.EMPTY_ELEMENT' | translate }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")

      ul.items-list(ng-repeat="type in elementsMenu.aux.addElementsList track by $index")
        .title(data-output="{{ elementsMenu.aux.addElementsType[$index] }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")

        li.item(ng-hide="elementsMenu.aux.isAddElementListView", ng-repeat="addElement in type track by $index", ng-class="{'active': elementsMenu.aux.isAddElement === $parent.$index+'-'+$index}", hm-tap="elementsMenu.chooseAddElement($parent.$index, $index)")
          .value(data-output="{{ addElement.elementName }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")
          button.element-info

        //------ for List View
        li.item(ng-show="elementsMenu.aux.isAddElementListView", ng-repeat="addElement in type track by $index", ng-class="{'active': elementsMenu.aux.isAddElement === $parent.$index+'-'+$index}", hm-tap="elementsMenu.selectElementListView($parent.$index, $index, $event)")
          .value(data-output="{{ addElement.elementName }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")
          button.element-info

    footer.menu-footer
      //.price-element(ng-show="global.isAddElement && elementsMenu.config.isAddElementPrice")
      .price-element(ng-show="elementsMenu.aux.isAddElement")
        .price-value + {{ elementsMenu.aux.currAddElementPrice }} {{ elementsMenu.userInfo.currency }}

      .elements-tools.size-calculator(ng-class="{'active': elementsMenu.global.isSizeCalculator}")
        include ../parts/_size-calculator
      .elements-tools.qty-calculator(ng-class="{'active': elementsMenu.global.isQtyCalculator}")
        include ../parts/_qty-calculator
      .elements-tools.color-selector(ng-class="{'active': elementsMenu.global.isColorSelector}")
        .color-label {{ 'add_elements_menu.COLOR_AVAILABLE' | translate }}

        .color-block
          .color-img(ng-class="{'selected': elementsMenu.aux.isAddElementColor === 'matt'}", hm-tap="elementsMenu.selectAddElementColor('matt')")
            img(ng-src="{{ elementsMenu.aux.addElementLaminatWhiteMatt.laminationUrl }}")
          .color-labels
            .aux-label(data-output="{{ elementsMenu.aux.addElementLaminatWhiteMatt.laminationName }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")
            .aux-label-bold(data-output="{{ elementsMenu.aux.addElementLaminatWhiteMatt.laminationLabel }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")

        .color-block
          .color-img(ng-class="{'selected': elementsMenu.aux.isAddElementColor === 'glossy'}", hm-tap="elementsMenu.selectAddElementColor('glossy')")
            img(ng-src="{{ elementsMenu.aux.addElementLaminatWhiteGlossy.laminationUrl }}")
          .color-labels
            .aux-label(data-output="{{ elementsMenu.aux.addElementLaminatWhiteGlossy.laminationName }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")
            .aux-label-bold(data-output="{{ elementsMenu.aux.addElementLaminatWhiteGlossy.laminationLabel }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")


        .color-block.color-options
          .color-option(ng-repeat="color in elementsMenu.aux.addElementLaminatColor")
            .color-img(ng-class="{'selected': elementsMenu.global.isAddElementColor === $index}", hm-tap="elementsMenu.selectAddElementColor($index)")
              img(ng-src="{{ color.laminationUrl }}")
            .aux-label(data-output="{{ color.laminationName }}", typing="{{ elementsMenu.config.typing }}", typing-delay="{{ elementsMenu.config.DELAY_START }}")



  .list-items(ng-show="elementsMenu.aux.isAddElementListView", ng-repeat="type in elementsMenu.aux.addElementsList track by $index")
    .list-item(ng-repeat="addElement in type track by $index")
      .aux-list-item(ng-class="{'selected': elementsMenu.aux.isAddElement == $parent.$index +'-'+ $index}", ng-style="elementsMenu.aux.coordinats")
        .aux-list-row
          .aux-name
            .aux-title {{ addElement.elementName }}
            .aux-label {{ 'add_elements.NAME_LABEL' | translate }}
          .aux-params(ng-if="addElement.elementQty")
