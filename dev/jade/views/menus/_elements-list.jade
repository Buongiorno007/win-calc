.elements-list-container(ng-controller="ElementsListCtrl", ng-class="[global.showAddElementsMenu, global.addElementsMenuStyle]")
  .elements-menu
    header.menu-header
      .tip(data-output="{{ 'add_elements_menu.TIP' | translate }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")
      button.close-elements-menu(ng-click="closeAddElementsMenu()")

    .menu-lists
      ul.items-list
        li.item.empty-element(ng-class="{'active': global.isAddElement == false}", ng-click="chooseAddElement()")
          .value(data-output="{{ 'add_elements_menu.EMPTY_ELEMENT' | translate }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")

      ul.items-list(ng-repeat="type in global.addElementsList track by $index")
        .title(data-output="{{ global.addElementsType[$index] }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")

        li.item(ng-hide="global.isAddElementListView", ng-repeat="addElement in type track by $index", ng-class="{'active': global.isAddElement === $parent.$index+'-'+$index}", ng-click="chooseAddElement($parent.$index, $index)")
          .value(data-output="{{ addElement.elementName }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")
          button.element-info

        li.item(ng-show="global.isAddElementListView", ng-repeat="addElement in type track by $index", ng-class="{'active': global.isAddElement === $parent.$index+'-'+$index}", ng-click="selectElementListView($parent.$index, $index, $event)")
          .value(data-output="{{ addElement.elementName }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")
          button.element-info

    footer.menu-footer
      .price-element(ng-show="addElementsMenu.isAddElementPrice")
        .price-value + {{ currAddElementPrice }}
        //price

      .elements-tools.size-calculator(ng-class="{'active': global.isSizeCalculator}")
        include ../parts/_size-calculator
      .elements-tools.qty-calculator(ng-class="{'active': global.isQtyCalculator}")
        include ../parts/_qty-calculator
      .elements-tools.color-selector(ng-class="{'active': global.isColorSelector}")
        .color-label {{ 'add_elements_menu.COLOR_AVAILABLE' | translate }}

        .color-block
          .color-img(ng-class="{'selected': global.isAddElementColor === 'matt'}", ng-click="selectAddElementColor('matt')")
            img(ng-src="{{ global.addElementLaminatWhiteMatt.laminationUrl }}")
          .color-labels
            .aux-label(data-output="{{ global.addElementLaminatWhiteMatt.laminationName }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")
            .aux-label-bold(data-output="{{ global.addElementLaminatWhiteMatt.laminationLabel }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")

        .color-block
          .color-img(ng-class="{'selected': global.isAddElementColor === 'glossy'}", ng-click="selectAddElementColor('glossy')")
            img(ng-src="{{ global.addElementLaminatWhiteGlossy.laminationUrl }}")
          .color-labels
            .aux-label(data-output="{{ global.addElementLaminatWhiteGlossy.laminationName }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")
            .aux-label-bold(data-output="{{ global.addElementLaminatWhiteGlossy.laminationLabel }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")


        .color-block.color-options
          .color-option(ng-repeat="color in global.addElementLaminatColor")
            .color-img(ng-class="{'selected': global.isAddElementColor === $index}", ng-click="selectAddElementColor($index)")
              img(ng-src="{{ color.laminationUrl }}")
            .aux-label(data-output="{{ color.laminationName }}", typing="{{ addElementsMenu.typing }}", typing-delay="{{ addElementsMenu.DELAY_START }}")



  .list-items(ng-show="global.isAddElementListView", ng-repeat="type in global.addElementsList track by $index")
    .list-item(ng-repeat="addElement in type track by $index")
      .aux-list-item(ng-class="{'selected': global.isAddElement == $parent.$index +'-'+ $index}", ng-style="addElementsMenu.coordinats")
        .aux-list-row
          .aux-name
            .aux-title {{ addElement.elementName }}
            .aux-label {{ 'add_elements.NAME_LABEL' | translate }}
          .aux-params(ng-if="addElement.elementQty")
            //button.aux-params-but.aux_big_but(ng-click="global.initAddElementMenuTools(1, $index)") {{ addElement.elementQty }}
            button.aux-params-but.aux_big_but {{ addElement.elementQty }}
            .aux-label {{ 'add_elements.QTY_LABEL' | translate }}
          .aux-params(ng-if="addElement.elementWidth")
            //button.aux-params-but.aux_big_but(ng-click="global.initAddElementMenuTools(2, $index)") {{ addElement.elementWidth }}
            button.aux-params-but.aux_big_but {{ addElement.elementWidth }}
            .aux-label {{ 'add_elements.SIZE_LABEL' | translate }}
          .aux-params(ng-if="addElement.elementHeight")
            button.aux-params-but.aux_big_but {{ addElement.elementHeight }}
            .aux-label {{ 'add_elements.SIZE_LABEL' | translate }}
          .aux-params(ng-if="addElement.elementColor")
            button.aux-color
              img(ng-src="{{ addElement.elementColor }}")
            .aux-label {{ 'add_elements.COLOR_LABEL' | translate }}

          .aux-price +{{ addElement.elementPrice }}
          button.aux-choose-but(ng-click="chooseAddElement($parent.$index, $index)") {{ 'add_elements.ADD' | translate }}



  // Tabs Grids
  .menu-right-tabs(ng-if="global.isFocusedAddElement === 1")
    .tab-block.simple-frame
      button.frame-tab(ng-class="{'active': global.chosenAddElements.selectedGrids.length == 1}") {{ 'add_elements_menu.TAB_NAME_SIMPLE_FRAME' | translate }}
    .tab-block.hard-frame(ng-class="{'active': global.isTabFrame}")
      button.frame-tab(ng-class="{'active': global.chosenAddElements.selectedGrids.length > 1}", ng-click="showFrameTabs()") {{ 'add_elements_menu.TAB_NAME_HARD_FRAME' | translate }}
      .tab-content-empty(ng-if="global.chosenAddElements.selectedGrids.length === 0")
        .tab-empty {{ 'add_elements_menu.TAB_EMPTY_EXPLAIN' | translate }}
      .tab-content(ng-if="global.chosenAddElements.selectedGrids.length > 0")
        .tab-aux-row(ng-repeat="addElement in global.chosenAddElements.selectedGrids track by $index")
          .aux-title {{ addElement.elementName }}


