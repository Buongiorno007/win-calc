                    .aux-params(ng-class="{'selected': global.auxParameter === '1-1-'+$index && global.currentAddElementId === $index}")
                      button.aux-params-but.aux_connect_but(ng-click="global.initAddElementMenuTools(1, $index)", ng-disabled="global.isQtyCalculator") {{ elementUnit.elementQty }}
                      .aux-label(data-output="{{ 'add_elements.QTY_LABEL' | translate }}", typing="{{ cart.typing }}", typing-delay="{{ cart.DELAY_START }}")
                  td.aux-tablecell-width
                  td.aux-tablecell-height
                  td.aux-tablecell-color
                  td.aux-tablecell-price
                    .aux-price + {{ elementUnit.elementQty * elementUnit.elementPrice }} {{ global.currency }}
                  td.aux-tablecell-delete
                    .aux-delete(ng-click="deleteAddElementList(elementUnit.elementType, elementUnit.elementId)")


