{
  "name": "StekoCalculator",
  "description": "Create your own coolest window",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "https://*/*",
    "http://*/*",
    "storage",
    "activeTab",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_icon": "img/login/logo-steko.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "img/login/logo-steko.png",
    "128": "img/login/logo-steko.png"
  }

}

